{
    "name": "beyondcode/laravel-visual-diff",
    "description": "Create visual diffs in your application tests.",
    "keywords": [
        "beyondcode",
        "visual regression",
        "laravel-visual-diff"
    ],
    "homepage": "https://github.com/beyondcode/laravel-visual-diff",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://beyondcode.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.22",
        "symfony/process": "^6.3",
        "spatie/browsershot": "^3.58"
    },
    "require-dev": {
        "orchestra/testbench-dusk": "^8.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\VisualDiff\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeyondCode\\VisualDiff\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeyondCode\\VisualDiff\\VisualDiffServiceProvider"
            ]
        }
    }
}
